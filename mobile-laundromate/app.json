{
  "expo": {
    "name": "mobile-laundromate",
    "slug": "mobile-laundromate",
    "scheme": "mobile-laundromate",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff",
      "fonts": {
        "Poppins-Regular": "./assets/fonts/Poppins-Regular.ttf",
        "Poppins-Bold": "./assets/fonts/Poppins-Bold.ttf",
        "Poppins-SemiBold": "./assets/fonts/Poppins-SemiBold.ttf",
        "Poppins-Medium": "./assets/fonts/Poppins-Medium.ttf",
        "Poppins-Light": "./assets/fonts/Poppins-Black.ttf"
      }
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    },

    "plugins": [
      "expo-router"
    ]
  }
}
